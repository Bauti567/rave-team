---
import Icon from '../Icon.astro'

interface Props {
  class?: string
  width?: number
  height?: number
}

const { class: extraClass, height = 18, width = 20 } = Astro.props
---

<Icon
  {width}
  {height}
  name="socials/bluesky"
  class:list={['class="w-8 hover:scale-125 duration-200', extraClass]}
/>
