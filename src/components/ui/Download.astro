---
import Icon from '../Icon.astro'

interface Props {
  class?: string
  width?: number
  height?: number
}

const { class: extraClass, height = 24, width = 24 } = Astro.props
---

<Icon
  {width}
  {height}
  name="download"
  class:list={['lucide lucide-download-icon lucide-download', extraClass]}
/>
