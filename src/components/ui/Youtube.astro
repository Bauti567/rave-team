---
import Icon from '../Icon.astro'

interface Props {
  class?: string
  width?: number
  height?: number
}

const { class: extraClass, height = 18, width = 24 } = Astro.props
---

<Icon
  {width}
  {height}
  name="socials/youtube"
  class:list={['w-8 duration-200 hover:scale-125', extraClass]}
/>
